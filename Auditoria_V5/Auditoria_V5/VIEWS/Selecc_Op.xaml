<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Auditoria_V5.Selecc_Op"
             Title="{Binding Almacen}" BackgroundColor="{StaticResource BackgroundColor}">
    <ContentPage.Content>
        <StackLayout Padding="5">

            <Button Text="Lista Ubicaciones" HeightRequest="100" BorderRadius="40" Clicked="Lista_Clicked" BackgroundColor="{StaticResource MainColor}"></Button>
            <Button Text="Filtro" HeightRequest="100" BorderRadius="40" BackgroundColor="{StaticResource MainColor}"></Button>
            <Button Text="Almacen Completo" HeightRequest="100" BorderRadius="40" Clicked="Almacen_Clicked" BackgroundColor="{StaticResource MainColor}"></Button>
            <Button Text="Exportar Resultados" HeightRequest="100" BorderRadius="40" Clicked="Export_Clicked" BackgroundColor="{StaticResource MainColor}"></Button>


        </StackLayout>
    </ContentPage.Content>
</ContentPage>